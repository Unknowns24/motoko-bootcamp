type Subaccount = blob;
type Result = 
 variant {
   err: text;
   ok;
 };
type MotoCoin = 
 service {
   airdrop: () -> (Result);
   balanceOf: (Account) -> (nat) query;
   name: () -> (text) query;
   symbol: () -> (text) query;
   totalSupply: () -> (nat);
   transfer: (Account, Account, nat) -> (Result);
 };
type Account = 
 record {
   owner: principal;
   subaccount: opt Subaccount;
 };
service : () -> MotoCoin
